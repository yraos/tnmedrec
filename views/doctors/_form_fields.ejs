<div class="form-row">
    <div class="form-item">
      <label>Cin</label>
      <input type="number" name="cin" value="<%= doctor.cin %>">
    </div>
    <div class="form-item">
      <label>First Name</label>
      <input type="text" name="first_name" value="<%= doctor.first_name %>">
    </div>
    <div class="form-item">
      <label>Last Name</label>
      <input type="text" name="last_name" value="<%= doctor.last_name %>">
    </div>
  </div>
  <div class="form-row">
    <div class="form-item">
      <label>Email</label>
      <input type="text" name="email" value="<%= doctor.email %>">
    </div>
    <div class="form-item">
      <label>Password</label>
      <input type="password" name="password" value="<%= doctor.password %>">
    </div>
    <div class="form-item">
      <label>Speciality</label>
      <input type="text" name="speciality" value="<%= doctor.speciality %>">
    </div>
  </div>
  <div class="form-row">
    <div class="form-item">
      <label>Phone Number</label>
      <input type="number" name="phone_number" value="<%= doctor.phone_number %>">
    </div>
    <div class="form-item">
      <label>Address</label>
      <input type="text" name="address" value="<%= doctor.address %>">
    </div>
  </div>